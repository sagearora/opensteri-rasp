{
    "name": "printer-management-system",
    "version": "1.0.38",
    "description": "A comprehensive Node.js application for managing GoDEX printers and label printing functionality using environment-based authentication",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc && cp -r src/views dist/",
        "start": "node dist/index.js",
        "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
        "codegen": "graphql-codegen --require dotenv/config --config codegen.yml",
        "codegen:watch": "graphql-codegen --require dotenv/config --config codegen.yml --watch",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "test": "jest",
        "clean": "rm -rf dist",
        "prebuild": "npm run clean",
        "version:patch": "node scripts/update-version.js patch",
        "version:minor": "node scripts/update-version.js minor",
        "version:major": "node scripts/update-version.js major",
        "version:patch:commit": "node scripts/update-version.js patch --commit",
        "version:minor:commit": "node scripts/update-version.js minor --commit",
        "version:major:commit": "node scripts/update-version.js major --commit"
    },
    "keywords": [
        "printer",
        "goddex",
        "graphql",
        "websocket",
        "label-printing",
        "raspberry-pi",
        "usb",
        "typescript"
    ],
    "author": "Printer Management System Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/your-org/printer-management-system.git"
    },
    "bugs": {
        "url": "https://github.com/your-org/printer-management-system/issues"
    },
    "homepage": "https://github.com/your-org/printer-management-system#readme",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "@types/node-fetch": "^2.6.12",
        "date-fns": "^4.1.0",
        "dotenv": "^17.2.1",
        "express": "^4.18.2",
        "graphql": "^16.11.0",
        "graphql-request": "^7.2.0",
        "graphql-ws": "^6.0.6",
        "node-fetch": "^2.7.0",
        "usb": "^2.16.0",
        "ws": "^8.18.3"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^5.0.3",
        "@graphql-codegen/cli": "^5.0.7",
        "@graphql-codegen/typescript": "^4.1.6",
        "@graphql-codegen/typescript-graphql-request": "^6.3.0",
        "@graphql-codegen/typescript-operations": "^4.6.1",
        "@graphql-codegen/typescript-resolvers": "^4.5.1",
        "@parcel/watcher": "^2.5.1",
        "@types/express": "^4.17.21",
        "@types/node": "^20.19.9",
        "@types/ws": "^8.18.1",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}
